    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Program",
            "type": "node",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "env": {},
            "runtimeExecutable": "/home/codespace/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-read"
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Test",
            "type": "node",
            "program": "${workspaceFolder}/Jackbennett/2024/main.ts",
            "cwd": "${workspaceFolder}/Jackbennett/2024",
            "env": {},
            "runtimeExecutable": "/home/codespace/.deno/bin/deno",
            "runtimeArgs": [
                "test",
                "--unstable",
                "--inspect-wait",
                "--allow-read"
            ],
            "attachSimplePort": 9229
        }
    ]
}